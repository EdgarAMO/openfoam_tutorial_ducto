FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;

z1      -3.00;
z2      3.000;
x1      -1.00;
x2      96.00;
y1      -3.00;
y2      61.00;    

vertices
(
    ($x1 $y1 $z1)
    ($x2 $y1 $z1)
    ($x2 $y2 $z1)
    ($x1 $y2 $z1)

    ($x1 $y1 $z2)
    ($x2 $y1 $z2)
    ($x2 $y2 $z2)
    ($x1 $y2 $z2)
);

blocks
(
	// the numbers nx ny nz must preserve the aspect ratio of the domain
    hex (0 1 2 3 4 5 6 7) (100 63 5) simpleGrading (1 1 1)
);

edges
(
);

boundary
(   
);

mergePatchPairs
(
);

